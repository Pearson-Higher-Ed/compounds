{
  "name": "Compounds",
  "version": "0.1.1",
  "description": "Atomic React Components",
  "main": "./build/dist.Compounds.js",
  "author": "Pearson Design Accelerator",
  "license": "MIT",
  "homepage": "http://pearson-higher-ed.github.io/compounds/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pearson-Higher-Ed/compounds"
  },
  "scripts": {
    "start": "npm run dev",
    "dev-setup": "node ./npm_scripts/dev-setup.js",
    "dev": "node ./npm_scripts/dev.js",
    "gh-pages": "npm run build&&gh-pages -d build",
    "pretest": "node ./npm_scripts/pretest.js",
    "test": "jest --watchAll --coverage",
    "build": "node ./npm_scripts/build.js",
    "lint": "node ./npm_scripts/lint.js",
    "gen-changelog": "node ./npm_scripts/gen-changelog.js",
    "version": "node ./npm_scripts/version.js",
    "ci": "node ./npm_scripts/ci.js",
    "release": "node ./npm_scripts/release.js",
    "verify": "node ./npm_scripts/verify.js",
    "copy-utils": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/cross-platform.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "**/src/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/test/**",
      "!webpack.config.js",
      "!**/demo/**",
      "!**/build/**",
      "!**/coverage/**",
      "!**/npm_scripts/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/utils/FileStub.js",
      "^.+\\.css$": "<rootDir>/test/utils/CSSStub.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules|demo|fonts|npm_scripts|coverage|build|npm_scripts)/"
    ],
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "react-intl": "~2.0.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "@pearson-components/npm-scripts": "^0.3.7",
    "babel-core": "^6.18.0",
    "babel-eslint": "~4.1.6",
    "babel-jest": "16.0.0",
    "babel-loader": "~6.2.0",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.7.2",
    "colors": "~1.1.2",
    "conventional-changelog": "~1.1.0",
    "css-loader": "~0.23.0",
    "enzyme": "^2.5.1",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.1.1",
    "eslint-plugin-react": "~3.16.1",
    "expect": "~1.18.0",
    "expect-jsx": "~2.5.1",
    "file-loader": "~0.8.5",
    "gh-pages": "^0.12.0",
    "ignore-styles": "~5.0.1",
    "intl": "~1.1.0",
    "jest": "16.0.2",
    "json-loader": "~0.5.4",
    "node-sass": "~3.7.0",
    "pearson-elements": "^1.1.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "sass-loader": "~3.2.0",
    "semver": "~5.1.0",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "uuid": "2.0.2",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0"
  },
  "keywords": [
    "pearson-components"
  ]
}
